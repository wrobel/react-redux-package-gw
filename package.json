  },
  "engines": {
    "node": "~6.2"
  },
  "scripts": {
    "clean:test": "rimraf test/helper",
    "clean:webpack": "rimraf webpack/lib",
    "build:test": "npm run clean:test && babel test/src/helper --out-dir test/helper -s",
    "build:webpack": "npm run clean:webpack && babel webpack/src/ --out-dir webpack/lib -s",
    "build": "npm run lint && npm run type && npm run build:test && npm run build:webpack",
    "lint": "eslint -c .eslintrc.js test/src webpack/src",
    "pack": "npm run build && npm pack",
    "type": "flow status",
    "type:full": "flow check"
  },
  "keywords": [
    "react",
    "redux",
    "test",
    "webpack",
    "styleguide",
    "config"
  ],
